{
  "name": "papicons-vue",
  "version": "0.1.5-vue.1",
  "description": "Jeu d'ic√¥nes Papillon pour Vue",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "rm -rf dist",
    "build": "bun build src/index.ts --outdir dist --minify && vue-tsc --project tsconfig.build.json",
    "generate": "bun scripts/generateVueIcons.ts",
    "generate:clean": "rm -rf src/icons"
  },
  "keywords": [
    "vue",
    "icons",
    "svg"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cleboost/papicons-vue.git"
  },
  "author": "Cleboost <clement.balarot@gmail.com> (https://github.com/Cleboost)",
  "bugs": {
    "url": "https://github.com/Cleboost/papicons-vue/issues"
  },
  "homepage": "https://github.com/Cleboost/papicons-vue#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@eslint/js": "^9.22.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.6.2",
    "svgo": "^3.3.2",
    "vue": "^3.5.13",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.1.6"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "workspaces": [
    "example"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "private": false
}